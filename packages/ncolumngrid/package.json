    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/jsdom": "^12.2.4",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "install-peers": "^1.0.3",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
