{
  "name": "@indot/error",
  "version": "0.3.3",
  "description": "INDOT Error React Component",
  "main": "dist/TMCError.js",
  "scripts": {
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:ts && npm run gen-docs && npm run no-jekyll",
    "build:types": "tsc --emitDeclarationOnly",
    "build:ts": "tsc",
    "gen-docs": "typedoc --readme README.md --excludeNotExported --ignoreCompilerErrors --exclude '{**/*spec*,**/node_modules/**,**/spec/**}' --out docs ./src",
    "no-jekyll": "touch docs/.nojekyll"
  },
  "keywords": [
    "React",
    "Material-UI",
    "Error"
  ],
  "author": "jarsmith@indot.in.gov",
  "license": "MIT",
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.35",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@indot/tmclogger": "0.0.3"
  },
  "gitHead": "cdd47eb2af0640ee9492e363a483a5349fee943c"
}
