{
  "name": "@indot/pagecontent",
  "version": "0.0.3",
  "description": "INDOT React content holder",
  "main": "dist/PageContent.js",
  "scripts": {
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:ts && npm run gen-docs && npm run no-jekyll",
    "build:types": "tsc --emitDeclarationOnly",
    "build:ts": "tsc",
    "gen-docs": "typedoc --readme README.md --excludeNotExported --ignoreCompilerErrors --exclude '{**/*spec*,**/node_modules/**,**/spec/**}' --out docs ./src",
    "no-jekyll": "touch docs/.nojekyll"
  },
  "keywords": [
    "React",
    "Material-UI"
  ],
  "author": "jarsmith@indot.in.gov",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "install-peers": "^1.0.3",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.0",
    "prop-types": "^15.7.2"
  }
}
