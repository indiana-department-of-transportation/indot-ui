{
  "name": "@indot/material-theme",
  "version": "0.0.4",
  "description": "Material UI theme for INDOT TMC applications.",
  "main": "dist/theme.js",
  "scripts": {
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:ts && npm run gen-docs",
    "build:types": "tsc --emitDeclarationOnly",
    "build:ts": "tsc",
    "build:docs": "typedoc --readme README.md --excludeNotExported --ignoreCompilerErrors --exclude '{**/*spec*,**/node_modules/**,**/spec/**}' --out docs ./src",
    "gen-docs": "npm run build:docs && npm run no-jekyll",
    "no-jekyll": "touch docs/.nojekyll"
  },
  "keywords": [
    "React",
    "Material-UI",
    "theme"
  ],
  "author": "jarsmith",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "install-peers": "^1.0.3",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "897417daa823d53b0bb348115ebb4b9e612de626"
}
