{
  "name": "@indot/alert",
  "version": "0.3.3",
  "description": "UIAlert component",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:ts && npm run gen-docs",
    "build:types": "tsc --emitDeclarationOnly",
    "build:ts": "tsc",
    "build:docs": "typedoc --readme README.md --excludeNotExported --ignoreCompilerErrors --exclude '{**/*spec*,**/node_modules/**,**/spec/**}' --out docs ./src",
    "gen-docs": "npm run build:docs && npm run no-jekyll",
    "no-jekyll": "touch docs/.nojekyll"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.9.8",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@material-ui/core": "^4.9.8",
    "@types/jest": "^24.0.18",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "jest": "^24.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-jest": "^24.1.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "jarsmith@indot.in.gov",
  "license": "MIT",
  "dependencies": {
    "@jasmith79/ts-utils": "^0.8.0"
  }
}
